{
  "name": "ghoster",
  "version": "1.0.0",
  "description": "Web api app to pass requests from guest vm to host.",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run copy-static-assets && npm run build-ts",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "copy-static-assets": "mkdir -p dist/ && cp -Rf src/config/ dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barndoor2276/ghoster.git"
  },
  "author": "bbergan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barndoor2276/ghoster/issues"
  },
  "homepage": "https://github.com/barndoor2276/ghoster#readme",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.6",
    "@types/winston": "^2.3.9",
    "nodemon": "^1.18.3",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "winston": "^3.0.0"
  }
}
