{
  "name": "ghoster",
  "version": "1.0.1",
  "description": "Web api application to pass requests from guest vm to host.",
  "main": "main.ts",
  "bin": "./dist/main.js",
  "scripts": {
    "start": "tsc-watch --onSuccess \"npm run serve\"",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "shx rm -Rf dist/*",
    "serve": "node dist/main.js"
  },
