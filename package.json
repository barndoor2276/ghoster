  },
  "homepage": "https://github.com/barndoor2276/ghoster#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/node": "^16.7.1",
    "@types/winston": "^2.3.9",
    "prettier": "^2.3.2",
    "shx": "^0.3.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.3.5"
  },
